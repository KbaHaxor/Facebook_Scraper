{
  "manifest_version": 2,
  "name": "Google Scholar Scraper",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["jquery.min.js", "background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "permissions": ["activeTab"]
}
