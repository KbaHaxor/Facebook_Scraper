{
  "manifest_version": 2,
  "name": "Facebook Scraper",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["jquery.min.js", "background.js", "urls.js"  ]
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": ["activeTab", "tabs"]
}
